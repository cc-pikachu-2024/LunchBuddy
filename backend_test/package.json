{
  "name": "lunchbuddybackendapi",
  "version": "1.0.0",
  "description": "Lambda function generated by Amplify",
  "main": "index.js",
  "license": "Apache-2.0",
  "scripts": {
    "test": "mocha tests/*.test.js",
    "rollback": "knex migrate:rollback --all",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "restore": "knex migrate:rollback --all && knex migrate:latest && knex seed:run",
    "restore2": "knex migrate:rollback --all && knex migrate:latest",
    "start": "node ./index.js",
    "dev": "nodemon ./index.js"
  },
  "dependencies": {
    "aws-serverless-express": "^3.3.5",
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "knex": "^3.1.0",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.92",
    "chai": "^5.1.2",
    "mocha": "^10.8.2",
    "nodemon": "^3.1.7"
  }
}
