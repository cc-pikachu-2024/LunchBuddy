{
  "name": "lunchbuddybackendapi",
  "version": "1.0.0",
  "description": "Lambda function generated by Amplify",
  "main": "index.js",
  "license": "Apache-2.0",
  "scripts": {
    "test": "NODE_ENV=development mocha tests/*.test.js",
    "migrate:dev": "NODE_ENV=development knex migrate:latest",
    "migrate:prod": "NODE_ENV=production knex migrate:latest",
    "seed:dev": "NODE_ENV=development knex seed:run",
    "seed:prod": "NODE_ENV=production knex seed:run",
    "start": "cross-env NODE_ENV=development nodemon app.js",
    "start:prod": "NODE_ENV=production nodemon app.js",
    "restore:dev": "NODE_ENV=development knex migrate:rollback --all && NODE_ENV=development knex migrate:latest && NODE_ENV=development knex seed:run",
    "restore:prod": "NODE_ENV=production knex migrate:rollback --all && NODE_ENV=production knex migrate:latest && NODE_ENV=production knex seed:run"
  },
  "dependencies": {
    "aws-serverless-express": "^3.3.5",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.17.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.15.2",
    "knex": "^3.1.0",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.92",
    "chai": "^4.5.0",
    "chai-http": "^4.4.0",
    "chai-subset": "^1.6.0",
    "cross-env": "^7.0.3",
    "mocha": "^10.8.2",
    "nodemon": "^3.1.7",
    "sinon": "^19.0.2"
  }
}
